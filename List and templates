(Standard Template Library using C++)
#include <iostream>
#include<vector>
#include<list>
using namespace std;
template<typename T>
void printli(list <T> & myvector) {
    if(myvector.empty()) return;
    for(T &i : myvector) cout << i << " ";
    cout << endl;
}
int main()
{
  list<int>myvector={1,2,3,4,5,6};
  //int sum=0;
  //int i;
 // myvector.push_back(300);
 // myvector.push_back(100);

  list<int>::iterator it1= myvector.begin();
  while((*++it1 != 2) && (it1 != myvector.end()));
if(it1!=myvector.end())
{
 myvector.insert(it1,100);}
 printli(myvector);
}
